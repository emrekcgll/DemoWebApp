
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>

    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">RestaurantWebApp</a>
            </div>
            <ul class="nav navbar-nav ">
                <li><a href="/Category/Index/">Category</a></li>
                <li><a href="/Food/Index/">Food</a></li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        Google Chart
                        <span class="caret"></span>
                    </a>
                    <ul class="dropdown-menu">
                        <li><a href="/Chart/Index/" class="dropbtn">Google Pie Chart</a></li>
                        <li><a href="/Chart/Index2/">Google Column Chart</a></li>
                        <li><a href="/Chart/Index3/">Google Dynamic Pie Chart</a></li>
                        <li><a href="/Chart/Index4/">Google Dynamic Column Chart</a></li>
                    </ul>
                </li>
                <li><a href="#">Popup</a></li>
                <li><a href="/Chart/Statistics/">Statistics</a></li>
                <li><a href="#">Settings</a></li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li><a href="#"><span class="glyphicon glyphicon-user"></span> Sign In</a></li>
                <li><a href="/Login/Logout/"><span class="glyphicon glyphicon-log-in"></span> Logout</a></li>
            </ul>
        </div>
    </nav>

    <div class="container">
        @RenderBody()
    </div>

</body>
</html>
